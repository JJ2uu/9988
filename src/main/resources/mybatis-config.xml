      <typeAlias type="com.tripleJ.gg88.domain.Member" alias="member"></typeAlias>
      <typeAlias type="com.tripleJ.gg88.domain.Page" alias="page"></typeAlias>
      <typeAlias type="com.tripleJ.gg88.domain.QnaReply" alias="qnaReply"></typeAlias>
      <typeAlias type="com.tripleJ.gg88.domain.Qna" alias="qna"></typeAlias>
      <typeAlias type="com.tripleJ.gg88.model.PagingDto" alias="pageDto"></typeAlias>
