      <typeAlias type="com.tripleJ.gg88.domain.QnaReplyVO" alias="qnaReplyVO"></typeAlias>
      <typeAlias type="com.tripleJ.gg88.domain.QnaVO" alias="qnaVO"></typeAlias>
