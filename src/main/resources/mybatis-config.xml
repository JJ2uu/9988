   <mappers>
      <mapper resource="mapper/dailyMapper.xml"/>
      <mapper resource="mapper/emergencyReplyMapper.xml"/>
      <mapper resource="mapper/emergencyMapper.xml"/>
      <mapper resource="mapper/exerciseMapper.xml"/>
      <mapper resource="mapper/foodMapper.xml"/>
      <mapper resource="mapper/mapMapper.xml"/>
      <mapper resource="mapper/memberMapper.xml"/>
      <mapper resource="mapper/qnaReplyMapper.xml"/>
      <mapper resource="mapper/qnaMapper.xml"/>
