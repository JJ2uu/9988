   <typeAliases>
      <typeAlias type="com.tripleJ.gg88.domain.Daily" alias="daily"></typeAlias>
      <typeAlias type="com.tripleJ.gg88.domain.EmergencyReply" alias="emergencyReply"></typeAlias>
      <typeAlias type="com.tripleJ.gg88.domain.Emergency" alias="emergency"></typeAlias>
      <typeAlias type="com.tripleJ.gg88.domain.Exercise" alias="exercise"></typeAlias>
      <typeAlias type="com.tripleJ.gg88.domain.Food" alias="food"></typeAlias>
